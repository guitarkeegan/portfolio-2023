// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Project {
  id  Int @id() @default(autoincrement())
  title String @unique
  authors String
  role String
  concept String
  process String
  challenges String
  technologies String
  liveUrl String?
  githubUrl String?
  demoUrl String?
  thumbnail String
  screenShot1 String?
  screenShot2 String?
  screenShot3 String?
  alt String
}
